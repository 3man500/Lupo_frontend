        tools:layout_editor_absoluteX="75dp"
        tools:layout_editor_absoluteY="16dp" />


    <ListView
        android:id="@+id/listView"
        android:layout_width="349dp"
        android:layout_height="513dp"
        android:layout_weight="1"
